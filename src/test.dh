int a = 10;

fun factorial(int n) {
    if (n <= 1) return 1
    return n * factorial(n - 1)
}

print factorial(a)  // 3628800

class Counter {
    init(){
        this.count = 0
    }

    increment() {
        this.count = this.count + 1
        print this.count
    }

    decrement(){
        this.count = this.count - 1
        print this.count
    }
}

var c = Counter()
c.increment()  // 1
c.decrement()
c.decrement()
c.decrement()
c.increment()  // 2
